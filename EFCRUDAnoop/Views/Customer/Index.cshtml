@model IEnumerable<EFCRUDAnoop.Models.Location>;

<h2>Locations</h2>
<table class="table table-bordered">
    <tr>
        <th>Location</th>
        <th>Details</th>
    </tr>
    @foreach (var location in Model)
    {
        <tr>
            <td>
                <a asp-action="CustomerList"
                   asp-controller="Customer"
                   asp-route-id="@location.Id">
                    @location.Place
            </a>
        </td>
        <td>@location.Detalis</td>
    </tr>
}
</table>